<h2>Add an audio<audio src=""></audio></h2>
<form action="/client/create" method="post" enctype="multipart/form-data">
<label for="title">
   Title:
   <input type="text" name="title" id="title">
</label>
<label for="file">
    Audio:
    <input type="file" name="file" id="file">
</label>

<button type="submit">Create</button>
</form>


<h2>All audios</h2>
{{#each audio}}

<audio controls >
    <source src={{file}} >
        </audio>
{{/each}}